    "# Showcase how optimini is used"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "1",
   "metadata": {},
   "outputs": [],
   "source": [
    "import optimini as om\n",
    "\n",
    "\n",
    "def my_fun(params):\n",
    "    return params[\"a\"] ** 2 + params[\"b\"] ** 2\n",
    "\n",
    "\n",
    "params = {\"a\": 1, \"b\": 2}\n",
    "res = om.minimize(my_fun, params, method=\"L-BFGS-B\")\n",
    "res.x"
   ]
  }
